{
    "compilerOptions": {
        "noEmit": true,
        "target": "ESNext",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "types": [],
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment"
    },
    "include": ["src"]
}
